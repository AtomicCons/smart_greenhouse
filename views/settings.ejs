<%- include('../partials/head') %>
<section class="settings">
  <form id="settingsForm">
    <div class="form-section-head">
      <h1>API</h1>
      <ul class="flex-container">
        <li>
          <label for="apiSelect">
            API
          </label>
          <select id="apiSelect">
            <option value="noValue">Please Select a Service from the dropdown</option>
            <option value="mailgun">
              Mailgun
            </option>
          </select>
          </li>
        <li>
          <label for="apiKey">
            Key
          </label>
          <input type="name" id="apiKey" disabled />
        </li>
        <li>
        <label for="apiDomain">
          Domain
        </label>
        <input type="text" id="apiDomain" disabled />
      </li>
      </ul>
    </div>

    <div class="form-section-head">
      <h1>Sensor</h1>
      <ul class="flex-container">
        <li>
          <label>
            Sensor
          </label>
          <select id="sensorSelect">
              <option>Mongoose/Config variable</option>
            <%  for(i in sensors.component.Multi){ %>
            <option value='<%=sensors.component.Multi[i] %>'>
              <%=sensors.component.Multi[i] %>
            </option>
            <%}%>
          </select>
        </li>
      </ul>
    </div>
  </form>
</section>



<%- include('../partials/nav') %>
<%- include('../partials/foot') %>
